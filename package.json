{
  "name": "weathermanager",
  "productName": "weathermanager",
  "version": "1.0.0",
  "description": "A weather manager to process mass weather station data",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "css": "npx tailwindcss -i ./src.css -o ./src/css/main.css --watch",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "PaddeCraft",
    "email": "70023807+PaddeCraft@users.noreply.github.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "weathermanager"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "chart.js": "^3.8.0",
    "electron-nunjucks": "^1.0.3",
    "electron-preferences": "^2.6.0",
    "electron-squirrel-startup": "^1.0.0",
    "fecha": "^4.2.3",
    "nunjucks": "^3.2.3"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@tailwindcss/typography": "^0.5.2",
    "daisyui": "^2.14.3",
    "electron": "18.2.0",
    "tailwindcss": "^3.0.24"
  }
}
